{
  "ignorePatterns": [
    {
      "pattern": "^http://khulnasoft.com"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/.attachments",
      "replacement": "https://khulnasoft.com/en/docs/.attachments"
    },
    {
      "pattern": "^/",
      "replacement": "https://khulnasoft.com/en/docs/"
    },
    {
      "pattern": "%20",
      "replacement": "-",
      "global": true
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://khulnasoft.com/en/docs"]
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 401, 403, 0]
}
